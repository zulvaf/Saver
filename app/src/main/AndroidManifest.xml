<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reducto.saver">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_aja"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.reducto.saver.SplashPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.reducto.saver.MainActivity"></activity>
        <activity android:name=".HomePageActivity"></activity>
        <activity android:name=".AddDeviceActivity"></activity>
        <activity android:name=".StatusPageActivity"></activity>
    </application>

</manifest>
